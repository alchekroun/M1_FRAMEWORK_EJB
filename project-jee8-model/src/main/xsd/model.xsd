<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc">


	<xsd:element name="user" type="user"></xsd:element>
	<xsd:element name="address" type="address"></xsd:element>
	<xsd:element name="ccinfo" type="ccinfo"></xsd:element>
	<xsd:element name="invoice" type="invoice"></xsd:element>
	<xsd:element name="invoiceWrapper" type="invoiceWrapper"></xsd:element>
	<xsd:element name="arret" type="arret"></xsd:element>
	<xsd:element name="train" type="train"></xsd:element>
	<xsd:element name="trainAvecResa" type="trainAvecResa"></xsd:element>
	<xsd:element name="trainSansResa" type="trainSansResa"></xsd:element>
	<xsd:element name="passager" type="passager"></xsd:element>

	<xsd:simpleType name="SimpleDate">
		<xsd:annotation>
			<xsd:appinfo>
				<xjc:javaType name="java.time.LocalDateTime"

					adapter="fr.pantheonsorbonne.ufr27.miage.model.jaxb.LocalDateTimeAdapter" />
			</xsd:appinfo>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern
				value="[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}" />
		</xsd:restriction>
	</xsd:simpleType>


	<xsd:complexType name="user">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:int"></xsd:element>
			<xsd:element name="fname" type="xsd:string"></xsd:element>
			<xsd:element name="lname" type="xsd:string"></xsd:element>
			<xsd:element name="membershipId" type="xsd:int"
				minOccurs="0"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="address">
		<xsd:sequence>
			<xsd:element name="streetName" type="xsd:string"></xsd:element>
			<xsd:element name="streetNumber" type="xsd:int"></xsd:element>
			<xsd:element name="zipCode" type="xsd:string"></xsd:element>
			<xsd:element name="Country" type="xsd:string"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:element name="freeTrialPlan">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="user" type="user"></xsd:element>
				<xsd:element name="address" type="address"></xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="ccinfo">
		<xsd:sequence>
			<xsd:element name="number" type="xsd:string" />
			<xsd:element name="validityDate" type="xsd:string" />
			<xsd:element name="ccv" type="xsd:int" />
		</xsd:sequence>
	</xsd:complexType>


	<xsd:complexType name="invoiceWrapper">
		<xsd:sequence>
			<xsd:element name="invoices" type="invoice"
				maxOccurs="unbounded"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="invoice">
		<xsd:sequence>
			<xsd:element name="date" type="SimpleDate">
			</xsd:element>
			<xsd:element name="contractId" type="xsd:int"></xsd:element>
			<xsd:element name="paid" type="xsd:boolean"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	
	
	
	<xsd:complexType name="train">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:int"></xsd:element>
			<xsd:element name="nom" type="xsd:string"></xsd:element>
			<xsd:element name="direction" type="arret"></xsd:element>
			<xsd:element name="direction_type" type="xsd:string"></xsd:element>
			<xsd:element name="numeroTrain" type="xsd:int"></xsd:element>
			<xsd:element name="reseau" type="xsd:string"></xsd:element>
			<xsd:element name="statut" type="xsd:string"></xsd:element>
			<xsd:element name="base_depart_temps" type="SimpleDate"></xsd:element>
			<xsd:element name="base_arrivee_temps" type="SimpleDate"></xsd:element>
			<xsd:element name="reel_depart_temps" type="SimpleDate"></xsd:element>
			<xsd:element name="reel_arrivee_temps" type="SimpleDate"></xsd:element>
			<xsd:element name="listeArrets" type="arret" maxOccurs="unbounded"></xsd:element>
			<xsd:element name="listePassager" type="passager" maxOccurs="unbounded"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="trainAvecResa">
  		<xsd:complexContent>
    		<xsd:extension base="train">
     			 <xsd:sequence>
       				 <xsd:element name="type" type="xsd:string" />
     			 </xsd:sequence>
   			 </xsd:extension>
  		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="trainSansResa">
  		<xsd:complexContent>
    		<xsd:extension base="train">
     			 <xsd:sequence>
       				 <xsd:element name="type" type="xsd:string" />
     			 </xsd:sequence>
   			 </xsd:extension>
  		</xsd:complexContent>
	</xsd:complexType>
	
	<xsd:complexType name="passager">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:int"></xsd:element>
			<xsd:element name="nom" type="xsd:string"></xsd:element>
			<xsd:element name="depart" type="arret"></xsd:element>
			<xsd:element name="arrive" type="arret"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	
	<xsd:complexType name="infoGare">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:int"></xsd:element>
			<xsd:element name="localisation" type="arret"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="infoCentre">
		<xsd:sequence>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="arret">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:int"></xsd:element>
			<xsd:element name="nom" type="xsd:string"></xsd:element>
			<xsd:element name="listeHeureDePassage" type="heureDePassage" maxOccurs="unbounded"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="perturbation">
		<xsd:sequence>
			<xsd:element name="id" type="xsd:int"></xsd:element>
			<xsd:element name="type" type="xsd:string"></xsd:element>
			<xsd:element name="perturbation_temps" type="SimpleDate"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="heureDePassage">
		<xsd:sequence>
			<xsd:element name="idTrain" type="xsd:int"></xsd:element>
			<xsd:element name="passage" type="SimpleDate"></xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	

</xsd:schema>